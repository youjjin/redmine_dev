<% html_title("Project tracking with EVM - Edit report ") %>
<h2><%= l(:title_evm_tab) %></h2>
<h3><%= l(:label_report_edit) %></h3>
<%= form_for @evm_report, url: {action: :update, method: :get} do |f| %>
  <%= render partial: "/evmreports/common",
      locals: { f: f,
                evm_report: @evm_report,
                project: @project,
                evmbaseline: @evmbaseline,
                evm_report_prev: @evm_report_prev,
                evm_setting: @emv_setting} %>
  <%= submit_tag l(:button_save) %>&emsp;
  <%= link_to l(:button_back), { controller: :evmreports, action: :index, id: @evm_report.project_id } %>
<% end %>
